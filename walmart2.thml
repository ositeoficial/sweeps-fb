<script>
function updateQuestion(currentId, nextId, circleNum) {
    const currentCircle = document.getElementById(`circle${circleNum}`);
    currentCircle.innerHTML = "";
    currentCircle.classList.add("completed");
    
    if (circleNum < 3) {
        const nextCircle = document.getElementById(`circle${circleNum + 1}`);
        nextCircle.classList.add("active");
    }

    const currentQuestion = document.getElementById(currentId);
    currentQuestion.classList.remove("active");
    currentQuestion.classList.add("left");

    setTimeout(() => {
        const nextQuestion = document.getElementById(nextId);
        nextQuestion.classList.add("active");
    }, 50);
}

function handleLocationClick() {
    updateQuestion("locationQuestion", "question1", 1);
}

function handleQuestion1(answer) {
    updateQuestion("question1", "question2", 2);
}

function handleQuestion2(answer) {
    updateQuestion("question2", "claimContainer", 3);
}
